@using StudentInfo.Web.Models
@model Applicant

@{
	ViewBag.Title = "Add New Applicant";
}

<h1>Add New Applicant</h1>

@using (Html.BeginForm())
{
	<div>First Name: @Html.TextBoxFor(a => a.FirstName)</div>
	<div>Last Name: @Html.TextBoxFor(a => a.LastName)</div>
	<div><input type="submit" value="Add" /></div>
}